def take_input():
  x= int(input("Enter a Number: "))
  find_fact(x)
def find_fact(x):
  factorial = 1
  if x<0 :
    print("Please Give a Positive Number")
  elif x==0 :
    print("1")
  else:
    for i in range(1,x+1):
      factorial*= i 
    print(f"Factorial of {x} is {factorial} ")
   

take_input()

